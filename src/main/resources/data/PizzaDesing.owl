            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozzarella and Tomato and Pepperoni toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#AnchovyTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#AnchovyTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#SeafoodTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#CaperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#CaperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#ChesseTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#ChesseTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#ChessyPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#ChessyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#ChesseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#DeepPanBase -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#PizzaBase"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#ThinAndCrispyBase"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#GreenPepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#GreenPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasSpiciness"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#Medium"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#HamTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#HamTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#MeatTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#HighCaloriePizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#HighCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasCaloricContent"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#InterestingPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#InterestingPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#PizzaTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#JalapenoPepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#JalapenoPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasSpiciness"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#Hot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#LowCaloriePizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#LowCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasCaloricContent"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#MargheritaPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#MargheritaPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozzarella and Tomato toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#MeatTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#MeatTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#MozzarellaTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#MozzarellaTopping">
