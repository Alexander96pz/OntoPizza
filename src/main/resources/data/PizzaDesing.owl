<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#"
     xml:base="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:pizza="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorial#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:PizzaTutorial="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial">
        <rdfs:comment>A tutorial ontology for the Pizza domain.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isBaseOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#PizzaBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasSpiciness">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#PizzaTopping"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasSpicinessPreference -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasSpicinessPreference">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Customer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isToppingOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isIngredientOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isMilderThan -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isMilderThan">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isSpicierThan"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isSpicierThan -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isSpicierThan">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Spiciness"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#purchasedByCustomer -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#purchasedByCustomer">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#purchasedPizza"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#purchasedPizza -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#purchasedPizza">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Customer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Pizza"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasCaloricContent -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasCaloricContent">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Pizza"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasDiscount -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasDiscount">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Customer"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasPhone -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasPhone">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#numberOfPizzasPurchased -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#numberOfPizzasPurchased">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Customer"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#ssn -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#ssn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Employee"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#AltaCaloriaPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#AltaCaloriaPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasCaloricContent"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Pizza altos en calorias</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#AmericanaHotPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#AmericanaHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasCaloricContent"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">500</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozzarella, Tomato, Jalapeno, and Pepperoni toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#AmericanaPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#AmericanaPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasCaloricContent"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">490</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozzarella, Tomato and Pepperoni toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#AnchovyTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#AnchovyTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#SeafoodTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#BajoCaloriaPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#BajoCaloriaPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasCaloricContent"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Pizzas bajo en calorias</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#BlobPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#BlobPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#MeatTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Cualquier Pizza que contenga Carne</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#CaperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#CaperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#VegetableTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasSpiciness"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Mild"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#CarnePizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#CarnePizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#HamTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasCaloricContent"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">650</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#CheeseTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#CheeseTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#PizzaTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasSpiciness"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Mild"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#CheesyPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#CheesyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Customer -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Customer">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Person"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#purchasedPizza"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#Pizza"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#hasPhone"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#DeepPanBase -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlonl/ontologies/2021/PizzaTutorial#PizzaBase"/>
