<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#"
     xml:base="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial">
        <rdfs:comment>A tutorial ontology for the Pizza domain</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isBaseOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#PizzaBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasSpiciness">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#PizzaTopping"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isToppingOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isIngredientOf"/>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isMilderThan -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isMilderThan">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isSpicierThan"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isSpicierThan -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isSpicierThan">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#Spiciness"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasCaloricContent -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasCaloricContent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#Pizza"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#AmericanaHotPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#AmericanaHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozzarella and Tomato and Pepperoni toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#AmericanaMediumPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#AmericanaMediumPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#GreenPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozzarella and Tomato and Pepperoni toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#AmericanaPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#AmericanaPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marlon/ontologies/2021/5/PizzaTutorial#NamedPizza"/>
